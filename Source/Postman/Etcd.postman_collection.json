				{
					"name": "Multi-Put",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"pm.globals.set('prefix_b64', CryptoJS.enc.Base64.stringify(CryptoJS.enc.Utf8.parse(",
									"    '/contact_id/'",
									")));",
									"pm.globals.set('true_b64', CryptoJS.enc.Base64.stringify(CryptoJS.enc.Utf8.parse(",
									"    'true'",
									")));"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"compare\":[\n        {\n            \"target\":\"CREATE\",\n            \"key\":\"{{prefix_b64}}\",\n            \"value\":\"{{true_b64}}\"\n        }\n    ],\n    \"success\":[\n        {\n            \"requestPut\":{\n                \"key\":\"Zm9v\",\n                \"value\":\"YmFy\"\n            }\n        },\n        {\n            \"requestPut\":{\n                \"key\":\"Zm9v\",\n                \"value\":\"YmFy\"\n            }\n        }\n    ]\n}",
							"options": {
