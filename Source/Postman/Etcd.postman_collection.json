				{
					"name": "Grant ContactID Permissions to /contact_id/*",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"pm.globals.set('key_b64', CryptoJS.enc.Base64.stringify(CryptoJS.enc.Utf8.parse(",
									"    '/contact_id/'",
									")));",
									"pm.globals.set('range_end_b64', CryptoJS.enc.Base64.stringify(CryptoJS.enc.Utf8.parse(",
									"    '/contact_id0'",
									")));"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"name\": \"contactid\",\n    \"perm\": {\n        \"key\": \"{{key_b64}}\",\n        \"permType\": \"READWRITE\",\n        \"range_end\": \"{{range_end_b64}}\"\n    }\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{ETCD_URL}}/v3/auth/role/grant",
							"host": [
								"{{ETCD_URL}}"
							],
							"path": [
								"v3",
