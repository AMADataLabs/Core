{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "lambda:InvokeFunction"
            ],
            "Resource": [
                "arn:aws:execute-api:us-east-1:arn:aws:lambda:us-east-1:644454719059:*/*/*/*",
                "arn:aws:lambda:us-east-1:arn:aws:lambda:us-east-1:644454719059:function:*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "ssm:GetParametersByPath",
                "ssm:GetParameters",
                "ssm:GetParameter"
            ],
            "Resource": [
                "arn:aws:ssm:us-east-1:arn:aws:lambda:us-east-1:644454719059:function:CPTGetClinicianDescriptors:parameter/DataLabs/DataLake/*",
                "arn:aws:ssm:us-east-1:arn:aws:lambda:us-east-1:644454719059:function:CPTGetClinicianDescriptors:parameter/DataLabs/CPT/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "kms:DescribeKey",
                "kms:GetKeyPolicy",
                "kms:GetKeyRotationStatus",
                "kms:GetPublicKey",
                "kms:ListKeys",
                "kms:ListAliases",
                "kms:ListKeyPolicies",
                "kms:Decrypt"
            ],
            "Resource": "arn:aws:kms:us-east-1:644454719059:key/*"
        }
    ]
}
