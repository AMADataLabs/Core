[
    {
        "name": "scheduler",
        "image": "${account}.dkr.ecr.${region}.amazonaws.com/${image}:${tag}",
        "cpu": 0,
        // # HADI ENV VARS HERE
        "environment": ["DYNAMODB_CONFIG_TABLE":],
        "essential": true,
        "volumesFrom": [],
        "ulimits": [
            {
                "name": "nofile",
                "softLimit": 65536,
                "hardLimit": 1048576
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-region": "${region}",
                "awslogs-group": "/ecs/scheduler/${environment}",
                "awslogs-stream-prefix": "ecs"
            }
        }
    }
]
