# Render with Script/render-template -t Deploy/OneView/etl_secrets.yaml.jinja -f Deploy/OneView/etl_secrets.yaml -v "ods_host=<ODS host>,ods_port=..."
---
data:
    ONEVIEW_ODS_HOST: '{{ods_host}}'  # dev: rdbd1156
    ONEVIEW_ODS_PORT: '{{ods_port}}'  # dev: 50000
    ONEVIEW_ODS_NAME: '{{ods_name}}'  # dev: devdw
    ONEVIEW_ODS_USERNAME: '{{ods_username}}'
    ONEVIEW_ODS_PASSWORD: '{{ods_password}}'

    ONEVIEW_SFTP_HOST: '{{sftp_host}}'  # dev: eft.ama-assn.org
    ONEVIEW_SFTP_USERNAME: '{{sftp_username}}'  # dev: datalabs
    ONEVIEW_SFTP_PASSWORD: '{{sftp_password}}'

    ONEVIEW_S3_ACCESS_KEY: '{{s3_access_key}}'
    ONEVIEW_S3_SECRET_KEY: '{{s3_secret_key}}'
    ONEVIEW_S3_INGESTED_DATA_BUCKET: '{{s3_ingested_bucket}}'  # dev: ama-hsg-datalabs-datalake-ingestion-onprem
    ONEVIEW_S3_PROCESSED_DATA_BUCKET: '{{s3_processed_bucket}}'  # dev: ama-hsg-datalabs-datalake-processing-onprem

    ONEVIEW_DATABASE_HOST: '{{database_host}}'  # dev: hsg-data-labs-db.hsg-data-labs-dev.svc.cluster.local
    ONEVIEW_DATABASE_PORT: '5432'
    ONEVIEW_DATABASE_BACKEND: 'postgresql+psycopg2'
    ONEVIEW_DATABASE_NAME: '{{database_name}}'  # dev: datalabs
    ONEVIEW_DATABASE_USERNAME:  '{{database_username}}'
    ONEVIEW_DATABASE_PASSWORD:  '{{database_password}}'
