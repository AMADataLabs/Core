    #######################################################################
    # DAG Variables - ({"DAG": {"S": "LICENSING"}, "Task": {"S": "DAG"}})
    #######################################################################

    LICENSING_SYNC__DAG__LAMBDA_FUNCTION: 'AIP-${ENVIRONMENT}-LicensingDAG'
    LICENSING_SYNC__DAG__DAG_CLASS: 'datalabs.etl.dag.intelligent_platform.licensing.sync.DAG'
