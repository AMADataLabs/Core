        CACHE_OUTPUT_FILES: 'manufacturer_code_mapping.csv'

    CREATE_LAB_TABLE:
        CACHE_CLASS:  ${S3_CACHE_CLASS}
        CACHE_BASE_PATH: ${S3_BASE_PATH}
        CACHE_BUCKET: ${S3_PROCESSED_DATA_BUCKET}
