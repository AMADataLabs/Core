        CACHE_BASE_PATH: ${S3_BASE_PATH}
        CACHE_BUCKET: ${S3_PROCESSED_DATA_BUCKET}
        CACHE_INPUT_FILES: 'Proprietary Laboratory Analyses (PLA) Codes/CPTPLA.csv'
        CACHE_OUTPUT_FILES: 'pla_details.csv'

    CREATE_MANUFACTURER_TABLE:
