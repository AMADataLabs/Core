        CACHE_BASE_PATH: ${S3_BASE_PATH}
        CACHE_BUCKET: ${S3_PROCESSED_DATA_BUCKET}
        CACHE_INPUT_FILES: 'MODUL.csv'
        CACHE_OUTPUT_FILES: 'modifier_type.csv'

    CREATE_MODIFIER_TABLE:
