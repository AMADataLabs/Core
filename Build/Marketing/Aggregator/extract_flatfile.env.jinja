# Repository Settings
DATALABS_PYTHONPATH='{{pythonpath}}'


# Project Settings
PROJECT_NAME='Marketing/Aggregator'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper

# Extractor
EXTRACTOR__TASK_CLASS=datalabs.etl.s3.extract.S3FileExtractorTask
EXTRACTOR__BUCKET=ama-dev-datalake-staged-us-east-1
EXTRACTOR__BASE_PATH="AMA/Marketing"
EXTRACTOR__INCLUDE_DATESTAMP="False"
EXTRACTOR__INCLUDE_NAMES='True'
EXTRACTOR__EXECUTION_OFFSET='days=1'


# Transformer
TRANSFORMER__TASK_CLASS=datalabs.etl.transform.PassThroughTransformerTask

# Loader
LOADER__TASK_CLASS=datalabs.etl.fs.load.LocalFileLoaderTask
LOADER__BASE_PATH="AMA/Marketing"
LOADER__FILES='flatfile.csv'
