# Repository Settings
DATALABS_PYTHONPATH='{{pythonpath}}'


# Project Settings
PROJECT_NAME='Marketing/Aggregator'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper


# Extractor
EXTRACTOR__TASK_CLASS=datalabs.etl.fs.extract.LocalFileExtractorTask
EXTRACTOR__BASE_PATH=./
EXTRACTOR__FILES='dataset_with_emails.csv, dataset_with_validation_dates.csv'


# Transformer
TRANSFORMER__TASK_CLASS=datalabs.etl.marketing.aggregate.transform.EmailValidationRequesterTask
TRANSFORMER__HOST={{host}}
TRANSFORMER__ACCOUNT={{account}}
TRANSFORMER__API_KEY={{api_key}}
TRANSFORMER__MAX_MONTHS='6'
TRANSFORMER__EXECUTION_TIME="2023-10-17T00:00:00"
TRANSFORMER__LEFT_MERGE_KEY='EMPPID'
TRANSFORMER__RIGHT_MERGE_KEY='id'


# Loader
LOADER__TASK_CLASS=datalabs.etl.fs.load.LocalFileLoaderTask
LOADER__BASE_PATH=./
LOADER__FILES='request_parameters.json,dated_dataset_with_emails.csv'
