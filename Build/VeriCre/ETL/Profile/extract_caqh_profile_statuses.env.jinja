# Repository Settings
DATALABS_PYTHONPATH='{{pythonpath}}'


# Project Settings
PROJECT_NAME='ETL'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper


# Extractor
EXTRACTOR__TASK_CLASS=datalabs.etl.fs.extract.LocalFileExtractorTask
EXTRACTOR__BASE_PATH=./
EXTRACTOR__FILES=caqh_profile_urls.txt


# PPD Transformer
TRANSFORMER__TASK_CLASS=datalabs.etl.http.extract.HTTPFileListExtractorTask
TRANSFORMER__USERNAME={{username}}
TRANSFORMER__PASSWORD={{password}}
TRANSFORMER__INCLUDE_NAMES=True


# Loader
LOADER__TASK_CLASS=datalabs.etl.fs.load.LocalFileLoaderTask
LOADER__BASE_PATH=./
LOADER__FILES=caqh_profile_statuses.pkl

