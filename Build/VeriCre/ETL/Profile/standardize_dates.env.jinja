# Repository Settings
DATALABS_PYTHONPATH='{{pythonpath}}'


# Project Settings
PROJECT_NAME='ETL'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper


# Extractor
EXTRACTOR__TASK_CLASS=datalabs.etl.fs.extract.LocalFileExtractorTask
EXTRACTOR__BASE_PATH=./
EXTRACTOR__FILES=ABMS_DATA.psv, DEA_DATA.psv, DEMOG_DATA.psv, LICENSE_DATA.psv, MED_SCH_DATA.psv, MED_TRAIN_DATA.psv, NPI_DATA.psv, SANCTIONS_DATA.psv

# PPD Transformer
TRANSFORMER__TASK_CLASS=datalabs.etl.manipulate.transform.DateFormatTransformerTask
TRANSFORMER__COLUMNS=ENUMERATION_DT, DEACTIVATION_DT, REACTIVATION_DT, RPTD_DT, END_DT, BIRTH_DT, DEATH_DT, PRA_EXPR_DT, DEA_AS_OF_DT, DEA_EXPR_DT, GRAD_DT, MATRICULATION_DT, EFFECTIVE_DT, EXPIRATION_DT, LAST_REPORTED_DT, BEGIN_DT, LIC_ISSUE_DT, LIC_EXP_DT, LIC_AS_OF_DT, LIC_RNW_DT
TRANSFORMER__INPUT_FORMAT=%M/%d/%Y
TRANSFORMER__SEPARATOR=|

# Loader
LOADER__TASK_CLASS=datalabs.etl.fs.load.LocalFileLoaderTask
LOADER__BASE_PATH=./
LOADER__FILES=ABMS_DATA_STD.psv, DEA_DATA_STD.psv, DEMOG_DATA_STD.psv, LICENSE_DATA_STD.psv, MED_SCH_DATA_STD.psv, MED_TRAIN_DATA_STD.psv, NPI_DATA_STD.psv, SANCTIONS_DATA_STD.psv



