# Repository Settings
DATALABS_PYTHONPATH='{{pythonpath}}'


# Project Settings
PROJECT_NAME='OneView'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper

# Specialty Merge extractor
EXTRACTOR_CLASS=datalabs.etl.s3.extract.S3UnicodeFileExtractorTask
EXTRACTOR_BASEPATH=AMA/OneView/%Y%m%d
EXTRACTOR_BUCKET=ama-hsg-datalabs-datalake-ingestion-onprem
EXTRACTOR_FILES=specialty.csv,ppd_physician.csv
EXTRACTOR_ACCESSKEY='{{key}}'
EXTRACTOR_SECRETKEY='{{secret}}'
EXTRACTOR_ENDPOINTURL=https://hsg-data-labs-minio.apps.openshifttest.ama-assn.org/minio

# Specialty Merge Transformer
TRANSFORMER_CLASS=datalabs.etl.oneview.reference.transform.SpecialtyMergeTransformerTask

# Specialty Merge Loader
LOADER_CLASS=datalabs.etl.s3.load.S3UnicodeTextFileLoaderTask
LOADER_ACCESS_KEY='{{key}}'
LOADER_SECRET_KEY='{{secret}}'
LOADER_BUCKET=ama-hsg-datalabs-datalake-ingestion-onprem
LOADER_ENDPOINT_URL=https://hsg-data-labs-minio.apps.openshifttest.ama-assn.org
LOADER_BASEPATH=AMA/OneView
LOADER_FILES='specialty_merged.csv'
