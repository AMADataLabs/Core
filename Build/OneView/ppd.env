# Repository Settings
DATALABS_PYTHONPATH='{{pythonpath}}'


# Project Settings
PROJECT_NAME='OneView'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper

EXTACTOR_DATABASE_NAME='eprdods'       # Test: etstods
EXTACTOR_DATABASE_USERNAME='{{ODS_username}}'
EXTACTOR_DATABASE_PASSWORD='{{ODS_password}}'
EXTACTOR_DATABASE_HOST='{{ODS_host}}'
EXTACTOR_DATABASE_PORT='54150'

# PPD Extractor
EXTRACTOR_CLASS=datalabs.etl.jdbc.extract.JDBCExtractor
EXTRACTOR_SQL="SELECT * FROM ODS.ODS_PPD_FILE;"
EXTRACTOR_DRIVER=com.ibm.db2.jcc.DB2Jcc
EXTRACTOR_DRIVER_TYPE=db2
EXTRACTOR_JAR_PATH='{{driver_jar_path}}'

# PPD Transformer
TRANSFORMER_CLASS=datalabs.etl.oneview.ppd.transform.PPDTransformer

# PPD Loader
LOADER_CLASS=datalabs.etl.s3.S3UnicodeTextFileLoaderTask
LOADER_ACCESS_KEY='{{key}}'
LOADER_SECRET_KEY='{{secret}}'
LOADER_BUCKET=ama-hsg-datalabs-datalake-ingestion-onprem
LOADER_ENDPOINT_URL=https://hsg-data-labs-minio.apps.openshifttest.ama-assn.org/minio
LOADER_BASEPATH=AMA/OneView/%Y%m%d
LOADER_FILES='ppd_physician.csv'
