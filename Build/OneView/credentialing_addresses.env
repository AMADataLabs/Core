# Repository Settings
DATALABS_PYTHONPATH='{{pythonpath}}'


# Project Settings
PROJECT_NAME='OneView'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper

# CredentialingAddresses Extractor
EXTRACTOR_CLASS=datalabs.etl.sftp.extract.SFTPFileExtractorTask
EXTRACTOR_BASEPATH="Data Analytics\Data-Science\Data\Masterfile_OneView"
EXTRACTOR_FILES=Org_Addresses.xlsx
EXTRACTOR_HOST=eft.ama-assn.org
EXTRACTOR_USERNAME='{{username}}'
EXTRACTOR_PASSWORD='{{password}}'

# CredentialingAddresses Transformer
TRANSFORMER_CLASS=datalabs.etl.transform.PassThroughTransformerTask

# CredentialingAddresses Loader
LOADER_CLASS=datalabs.etl.s3.load.S3FileLoaderTask
LOADER_ACCESSKEY='{{key}}'
LOADER_SECRETKEY='{{secret}}'
LOADER_BUCKET=ama-hsg-datalabs-datalake-ingestion-onprem
LOADER_BASEPATH=AMA/OneView
LOADER_ENDPOINTURL=https://hsg-data-labs-minio.apps.openshifttest.ama-assn.org
LOADER_FILES='credentialing_addresses.xlsx'
