# Repository Settings
DATALABS_PYTHONPATH='{{pythonpath}}'


# Project Settings
PROJECT_NAME='OneView'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper

# RaceEthnicity Extractor
EXTRACTOR_CLASS=datalabs.etl.sftp.extract.SFTPUnicodeTextFileExtractorTask
EXTRACTOR_BASEPATH="Data Analytics\Data-Science\Data\Masterfile_OneView"
EXTRACTOR_FILES=PhysicianRaceEthnicity.csv
EXTRACTOR_HOST=eft.ama-assn.org
EXTRACTOR_USERNAME='{{username}}'
EXTRACTOR_PASSWORD='{{password}}'

# RaceEthnicity Transformer
TRANSFORMER_CLASS=datalabs.etl.oneview.race_ethnicity.transform.RaceEthnicityTransformer

# RaceEthnicity Loader
LOADER_CLASS=datalabs.etl.s3.load.S3UnicodeTextFileLoaderTask
LOADER_ACCESS_KEY='{{key}}'
LOADER_SECRET_KEY='{{secret}}'
LOADER_BUCKET=ama-hsg-datalabs-datalake-ingestion-onprem
LOADER_BASEPATH=AMA/OneView/%Y%m%d
LOADER_ENDPOINT_URL=https://hsg-data-labs-minio.apps.openshifttest.ama-assn.org/minio
LOADER_FILES='race_ethnicity.csv'
