# Repository Settings
DATALABS_PYTHONPATH='{{pythonpath}}'


# Project Settings
PROJECT_NAME='OneView'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper

# Extractor
EXTRACTOR__TASK_CLASS=datalabs.etl.sftp.extract.SFTPFileExtractorTask
EXTRACTOR__HOST='https://hsg-data-labs-minio.apps.openshifttest.ama-assn.org'
EXTRACTOR__BASE_PATH=AMA/OneView
EXTRACTOR__USERNAME={{user}}
EXTRACTOR__PASSWORD={{password}}
EXTRACTOR__FILES='ppd_physician.csv,physician_npi.csv,race_ethnicity.csv'
EXTRACTOR__BUCKET={{bucket}}
EXTRACTOR__ON_DISK=True

# PPD Transformer
TRANSFORMER__TASK_CLASS=datalabs.etl.oneview.ppd.transform.PPDTransformerTask

# PPD Loader
LOADER__TASK_CLASS=datalabs.etl.s3.load.S3UnicodeTextFileLoaderTask
LOADER__ACCESS_KEY={{key}}
LOADER__SECRET_KEY={{secret}}
LOADER__BUCKET=ama-{{bucket}}
LOADER__BASE_PATH=AMA/OneView
LOADER__FILES=ppd_physician.csv
