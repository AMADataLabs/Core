# Repository Settings
DATALABS_PYTHONPATH='{{pythonpath}}'


# Project Settings
PROJECT_NAME='OneView'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper

# CredentialingCustomerInstitution Extractor
EXTRACTOR_CLASS=datalabs.etl.s3.extract.S3FileExtractorTask
EXTRACTOR_BASEPATH=AMA/OneView
EXTRACTOR_BUCKET=ama-hsg-datalabs-datalake-ingestion-onprem
EXTRACTOR_FILES=credentialing_customer_merged.csv,rresidency_program_institution.csv
EXTRACTOR_ACCESSKEY='{{key}}'
EXTRACTOR_SECRETKEY='{{secret}}'
EXTRACTOR_ENDPOINTURL=https://hsg-data-labs-minio.apps.openshifttest.ama-assn.org/

# CredentialingCustomerInstitution Transformer
TRANSFORMER_CLASS=datalabs.etl.oneview.link.transform.CredentialingCustomerInstitutionTransformerTask

# CredentialingCustomerInstitution Loader
LOADER_CLASS=datalabs.etl.s3.load.S3UnicodeTextFileLoaderTask
LOADER_ACCESSKEY='{{key}}'
LOADER_SECRETKEY='{{secret}}'
LOADER_BUCKET=ama-hsg-datalabs-datalake-ingestion-onprem
LOADER_BASEPATH=AMA/OneView
LOADER_ENDPOINTURL=https://hsg-data-labs-minio.apps.openshifttest.ama-assn.org
LOADER_FILES=credentialing_customer_institution.csv
