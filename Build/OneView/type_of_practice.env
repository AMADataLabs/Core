# Repository Settings
DATALABS_PYTHONPATH='{{pythonpath}}'


# Project Settings
PROJECT_NAME='OneView'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper

EXTRACTOR_DATABASE_NAME='aims_prod:prd1srvxnet'
EXTRACTOR_DATABASE_USERNAME='{{AIMS_username}}'
EXTRACTOR_DATABASE_PASSWORD='{{AIMS_password}}'
EXTRACTOR_DATABASE_HOST='{{AIMS_host}}'
EXTRACTOR_DATABASE_PORT='22093'

# TypeOfPractice DB2 Extractor
EXTRACTOR_CLASS=datalabs.etl.jdbc.extract.JDBCExtractorTask
EXTRACTOR_SQL="SELECT DISTINCT T.top_cd, T.description FROM informix.top_pr T;"
EXTRACTOR_DRIVER=com.informix.jdbc.IfxDriver
EXTRACTOR_DRIVERTYPE=informix-sqli
EXTRACTOR_JARPATH=./jdbc-4.50.2.fix-1.jar,./bson-3.8.0.jar

# TypeOfPractice DB2 Transformer
TRANSFORMER_CLASS=datalabs.etl.oneview.reference.transform.TypeOfPracticeTransformerTask

# TypeOfPractice DB2 Loader
LOADER_CLASS=datalabs.etl.s3.load.S3UnicodeTextFileLoaderTask
LOADER_ACCESSKEY='{{key}}'
LOADER_SECRETKEY='{{secret}}'
LOADER_BUCKET=ama-hsg-datalabs-datalake-ingestion-onprem
LOADER_BASEPATH=AMA/OneView
LOADER_ENDPOINTURL=https://hsg-data-labs-minio.apps.openshifttest.ama-assn.org
LOADER_FILES='type_of_practice.csv'
