# Repository Settings
DATALABS_PYTHONPATH='{{ pythonpath }}'


# Project Settings
PROJECT_NAME='OneView'


TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper

# OneView Final S3 extractor
EXTRACTOR_CLASS=datalabs.etl.s3.extract.S3UnicodeFileExtractorTask
EXTRACTOR_BASEPATH=AMA/OneView/%Y%m%d
EXTRACTOR_BUCKET=ama-hsg-datalabs-datalake-ingestion-onprem
EXTRACTOR_FILES='{{files}}'
EXTRACTOR_ACCESSKEY='{{key}}'
EXTRACTOR_SECRETKEY=''{{secret}}''
EXTRACTOR_ENDPOINTURL=https://hsg-data-labs-minio.apps.openshifttest.ama-assn.org/minio

# OneView Final Transformer
TRANSFORMER_CLASS=datalabs.etl.transform.PassThroughTransformerTask


# OneView Final DB2 Loader
LOADER_CLASS=datalabs.etl.orm.load.ORMLoader
LOADER_MODELCLASSES='{{table_model_class}}'
LOADER_DATABASE_NAME='oneview'
LOADER_DATABASE_BACKEND=postgresql+psycopg2
LOADER_DATABASE_HOST='10.104.16.31'
LOADER_DATABASE_USERNAME='{{database_username}}'
LOADER_DATABASE_PASSWORD='{{database_password}}'
LOADER_TABLES='{{table_name}}'
