TRANSFORMER__FORMATTERS='datalabs.curate.cpt.descriptor.LongFixedWidthDescriptorFormatter,datalabs.curate.cpt.descriptor.MediumFixedWidthDescriptorFormatter,datalabs.curate.cpt.descriptor.ShortFixedWidthDescriptorFormatter'

# Loader
LOADER__TASK_CLASS=datalabs.etl.fs.load.LocalFileLoaderTask
