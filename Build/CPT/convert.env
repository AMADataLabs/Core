# Repository Settings
DATALABS_PYTHONPATH={{ pythonpath }}


# ProjectSettings
PROJECT_NAME=CPT


# CPTConvert ETL Settings
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapper
TASK_CLASS=datalabs.etl.task.ETLTask

EXTRACTOR__TASK_CLASS=datalabs.etl.cpt.ingest.extract.CPTTextDataExtractorTask
EXTRACTOR__BUCKET=ama-hsg-datalabs-datalake-ingestion-sandbox
EXTRACTOR__BASE_PATH=AMA/CPT
EXTRACTOR__FILES='standard/SHORTU.txt,standard/MEDU.txt,standard/LONGULT.txt,standard/MODUL.txt,standard/Consumer Friendly Descriptors/ConsumerDescriptor.txt,standard/Clinician Descriptors/ClinicianDescriptor.txt,standard/Proprietary Laboratory Analyses (PLA) Codes/CPTPLA,CPT Link/history/Deleted_DTK_tab.txt,CPT Link/history/HistoryModifiers_DTK_tab.txt,CPT Link/history/History_DTK_tab.txt'
