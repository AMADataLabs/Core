# Repository Settings
DATALABS_PYTHONPATH={{ pythonpath }}


# ProjectSettings
PROJECT_NAME=CPT


# CPTBundlePDF ETL Settings
TASK_WRAPPER_CLASS=datalabs.etl.awslambda.ETLTaskWrapperTask
