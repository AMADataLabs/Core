TASK_CLASS=datalabs.etl.task.ETLTask
TASK_WRAPPER_CLASS=datalabs.etl.task.ETLTaskWrapper


# Extractor
EXTRACTOR__TASK_CLASS=datalabs.etl.s3.extract.S3FileExtractorTask
EXTRACTOR__REGION_NAME='us-east-1'
EXTRACTOR__ACCESS_KEY='{{key}}'
EXTRACTOR__SECRET_KEY='{{secret}}'
EXTRACTOR__BUCKET=ama-dev-datalake-ingest-us-east-1
EXTRACTOR__BASE_PATH='AMA/operations/address/ppma/license_movement'
EXTRACTOR__FILES='results.txt'
EXTRACTOR__ASSUME_ROLE=arn:aws:iam::191296302136:role/dev-ama-apigateway-invoke-role
